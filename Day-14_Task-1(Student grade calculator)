import json
def calculate_grade(avg):
    if avg>=90:
        return "A"
    elif avg>=75:
        return"B"
    elif avg>=60:
        return"C"
    elif avg>=40:
        return"D"
    else:
        return"F"
student=[]
name=input("Enter student name: ")
marks=[]
for i in range(5):
   m =int(input(f"Enter your marks of subjects,{i+1} : "))
   marks.append(m)
total=sum(marks)
avg=total/5
grade=calculate_grade(avg)
student_data={
    "name":name,
    "marks":marks,
    "total":total,
    "average":avg,
    "grade":grade
}
student.append(student_data)
